    "section_liquid_KWtGT3": {
      "type": "section-liquid",
      "settings": {
        "custom_liquid": "<script>\n    document.addEventListener('DOMContentLoaded', function () {\n        //console.log('DOMContentLoaded event fired');\n        \n        const seatRadios = document.querySelectorAll('input[name=\"options[babyark Convertible Car Seat - Premium (Color)]\"]');\n        //console.log('Seat radios found:', seatRadios.length);\n         \n        const cupHolderRadios = document.querySelectorAll('input[name=\"options[The babyark Smart Base ™ (Color)]\"]');\n        //console.log('Cup holder radios found:', cupHolderRadios.length);\n        \n        const specialMsg = document.querySelector('#specialMsg b');\n        //console.log('Special message element found:', !!specialMsg);\n    \n        function checkBaseColor() {\n            //console.log('checkBaseColor function called');\n            try {\n                // Get the selected seat option\n                const selectedSeatElement = document.querySelector('input[name=\"options[babyark Convertible Car Seat - Premium (Color)]\"]:checked');\n                //console.log('Selected seat element:', selectedSeatElement);\n                \n                if (!selectedSeatElement) {\n                    //console.log('No seat option selected');\n                    return;\n                }\n                \n                const selectedSeat = selectedSeatElement.value;\n                //console.log('Selected seat value:', selectedSeat);\n                \n                // Extract the base color from the seat option\n                const seatBaseColor = selectedSeat.split(' / ')[0].toLowerCase();\n                //console.log('Seat base color:', seatBaseColor);\n    \n                // Get the selected cup holder option\n                const selectedCupHolderElement = document.querySelector('input[name=\"options[The babyark Smart Base ™ (Color)]\"]:checked');\n                //console.log('Selected cup holder element:', selectedCupHolderElement);\n                \n                if (!selectedCupHolderElement) {\n                    //console.log('No cup holder option selected');\n                    return;\n                }\n                \n                const selectedCupHolder = selectedCupHolderElement.value.toLowerCase();\n                //console.log('Selected cup holder color:', selectedCupHolder);\n    \n                // Compare the colors\n                //console.log('Colors match?', seatBaseColor === selectedCupHolder);\n    \n                if (seatBaseColor !== selectedCupHolder) {\n                    //console.log('Showing mismatch message');\n                    specialMsg.innerHTML = `\n                        <svg width=\"20\" height=\"20\" viewBox=\"0 0 20 20\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                            <path d=\"M10.0003 18.3332C14.6027 18.3332 18.3337 14.6022 18.3337 9.99984C18.3337 5.39746 14.6027 1.6665 10.0003 1.6665C5.39795 1.6665 1.66699 5.39746 1.66699 9.99984C1.66699 14.6022 5.39795 18.3332 10.0003 18.3332Z\" fill=\"#E7B264\"/>\n                            <path d=\"M10 6.6665V9.99984\" stroke=\"#304545\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n                            <path d=\"M10 13.3335H10.0083\" stroke=\"#304545\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n                        </svg>\n                        <span>Note: Base and seat color do not match.</span>\n                    `;\n                } else {\n                    //console.log('Clearing mismatch message');\n                    specialMsg.innerHTML = '';\n                }\n            } catch (error) {\n                console.error('Error in checkBaseColor:', error);\n            }\n        }\n    \n        // Attach event listeners\n        seatRadios.forEach(radio => {\n            radio.addEventListener('change', () => {\n                //console.log('Seat radio changed:', radio.value);\n                checkBaseColor();\n            });\n        });\n        \n        cupHolderRadios.forEach(radio => {\n            radio.addEventListener('change', () => {\n                //console.log('Cup holder radio changed:', radio.value);\n                checkBaseColor();\n            });\n        });\n    \n        // Initial check\n        //console.log('Performing initial check');\n        checkBaseColor();\n    });\n    </script>\n    \n    <style>\n    #specialMsg svg{\n    width:40px;}\n    @media (min-width:1140px){\n    .image-with-text-new .iwt-image{\n    min-height:90vh;\n    }\n    .image-with-text-new .iwt-image> .relative{\n    padding-bottom:90vh;\n    }\n    .image-with-text-new .iwt-image> .relative > div{\n    min-height:90vh}\n    }\n    @media (max-width:480px){\n    \n    .image-with-text-new .iwt-image{\n    min-height:100vw;\n    } \n    .image-with-text-new .iwt-image> .relative{\n    padding-bottom:100vw;\n    }\n    .image-with-text-new .iwt-image> .relative > div{\n    min-height:100vw}\n    }\n    }\n    </style>"
