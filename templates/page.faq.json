    "section_liquid_bCgRkM": {
      "type": "section-liquid",
      "settings": {
        "custom_liquid": "<style>\n.section-faq > .wrapper--narrow{\n    max-width: 55%;\n    margin-left: calc(100vw - 90%);\n}\n@media (max-width:768px){\n.section-faq > .wrapper--narrow{\n    max-width: 100%;\nmargin-left:0;\n}\n}\n#faqMenu {\n        position: fixed;\n        top: 50%;\n        transform: translateY(-50%);\n        left: 960px;\n        background-color: transparent;\n        padding: 0 20px;\n    }\n    .faq-menu a {\n        display: block;\n        margin-bottom: 10px;\n        font-size: 22px /* Additional styling */;\n    }\n    .faq-menu > a.active {\n        font-weight: 600;\n        border-bottom: 1px solid rgba(0, 0, 0, 0.5);\n    }\n</style>\n<div id=\"faqMenu\" class=\"faq-menu\">\n    <!-- Menu items will be added here by JavaScript -->\n</div>\n<script>\n\ndocument.addEventListener('DOMContentLoaded', function() {\n    // Create anchor menu for FAQ sections\n    const faqMenu = document.getElementById('faqMenu');\n    const faqSections = document.querySelectorAll('.section-faq .text__standard');\n    const faqLinks = [];\n    faqSections.forEach((section, index) => {\n        const id = 'section-faq-' + index;\n        section.id = id; // Set ID on .text__standard\n        const h2 = section.querySelector('p.standard__kicker');\n        const link = document.createElement('a');\n        link.href = '#' + id;\n        link.textContent = h2.textContent;\n        faqMenu.appendChild(link);\n        faqLinks.push(link);\n    });\n\n    // Adjust menu position\n    const firstFaqWrapperWidth = 790; // Fixed width of the first .faq-wrapper\n    const stickyHeaderHeight = document.querySelector('.header__inner').offsetHeight;\n    const calculateMenuPosition = () => {\n        const windowWidth = window.innerWidth;\n        const leftValue = windowWidth > 1300 ? (windowWidth - 1300) / 2 + 160 + 90 + firstFaqWrapperWidth : 90 + firstFaqWrapperWidth;\n        //const topValue = document.querySelector('.section-faq p.standard__kicker').getBoundingClientRect().top + window.pageYOffset;\n\n        faqMenu.style.left = leftValue + 'px';\n        //faqMenu.style.top = topValue + 'px';\n    };\n    calculateMenuPosition();\n    window.addEventListener('resize', calculateMenuPosition);\n\n    // Smooth scrolling\n    document.documentElement.style.scrollBehavior = 'smooth';\n\n    // Highlight menu item based on scroll position\n    const highlightMenu = () => {\n    let currentSectionId = '';\n    let lastSectionId = faqSections[faqSections.length - 1].id;\n\n    faqSections.forEach(section => {\n        const sectionTop = section.getBoundingClientRect().top + window.pageYOffset - stickyHeaderHeight;\n        const sectionHeight = section.offsetHeight + 160;\n        if (window.pageYOffset >= sectionTop - 680 && window.pageYOffset < sectionTop + sectionHeight - 680) {\n            currentSectionId = section.id;\n        }\n    });\n\n    // Determine if the user has scrolled to the bottom of the page\n    const scrolledToBottom = window.innerHeight + window.scrollY >= document.body.offsetHeight;\n\n    faqLinks.forEach(link => {\n        link.classList.remove('active');\n        if (link.href.includes(currentSectionId) || (scrolledToBottom && link.href.includes(lastSectionId))) {\n            link.classList.add('active');\n        }\n    });\n};\nwindow.addEventListener('scroll', highlightMenu);\nhighlightMenu(); // Run on load to set initial active link\n\n\n});\n\n\ndocument.addEventListener('DOMContentLoaded', function() {\n    const offset = 110; // Offset from the target section\n\n    // Function to handle anchor click and smooth scroll\n    const handleAnchorClick = function(event) {\n        const href = this.getAttribute('href');\n        if (href.startsWith('#')) {\n            event.preventDefault();\n            const targetId = href.substring(1);\n            const targetElement = document.getElementById(targetId);\n\n            if (targetElement) {\n                const targetPosition = targetElement.getBoundingClientRect().top + window.pageYOffset - offset;\n                \n                window.scrollTo({\n                    top: targetPosition,\n                    behavior: 'smooth'\n                });\n            }\n        }\n    };\n\n    // Add event listeners to all anchors starting with '#'\n    const anchors = document.querySelectorAll('a[href^=\"#\"]');\n    anchors.forEach(anchor => {\n        anchor.addEventListener('click', handleAnchorClick);\n    });\n});\n\n</script>"
      }
